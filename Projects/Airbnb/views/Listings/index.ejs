<% layout("/layouts/boilerplate") %>

    <body>
        <div class="row row-cols-lg-3 row-cols-md-2 row-cols-sm-1">
            
            <% for(let listing of allListings){ %>

        
                <div class="card col" style="width: 20rem;">
                    <img src="<%= listing.image.url%>" class="card-img-top" alt="Image of places">
                    <div class="card-body">
                        <p class="card-text">
                            <%= listing.title %>
                            <br>
                           &#8377;  <%= listing.price %>/Night
                        </p>
                    </div>
                </div>

                <% } %>
        </div>


        

    </body>